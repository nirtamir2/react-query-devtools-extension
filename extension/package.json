{
  "name": "react-query-devtools-extension",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite",
    "format": "prettier **/* --write --ignore-unknown",
    "lint": "pnpm lint-js && pnpm lint-style",
    "lint-js": "eslint --fix **/*.{ts,tsx}",
    "lint-style": "stylelint --fix **/*.css",
    "preview": "vite preview",
    "start:chromium": "web-ext run --start-url http://localhost:3001 --source-dir ./dist --target=chromium",
    "start:firefox": "web-ext run --start-url http://localhost:3001 --source-dir ./dist --target=firefox-desktop",
    "type-check": "tsc"
  },
  "dependencies": {
    "match-sorter": "^6.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "webextension-polyfill": "^0.8.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.13",
    "@types/webextension-polyfill": "^0.8.2",
    "@vitejs/plugin-react": "^1.2.0",
    "prettier": "^2.5.1",
    "prettier-plugin-packagejson": "^2.2.15",
    "rollup-plugin-chrome-extension": "^4.0.1-16",
    "typescript": "^4.6.2",
    "vite": "^2.8.6",
    "web-ext": "^6.7.0",
    "webext-bridge": "^5.0.3"
  },
  "engines": {
    "node": ">=14.0.0",
    "pnpm": ">=6.24.0"
  }
}
